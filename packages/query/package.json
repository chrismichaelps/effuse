		"server-state",
		"query-client",
		"invalidate-queries",
		"refetch",
		"fine-grained",
		"signals",
		"reactive",
		"typescript",
		"type-safe",
		"effect-ts",
		"structured-concurrency"
	],
	"engines": {
		"node": ">=23.0.0"
	},
	"peerDependencies": {
		"@effuse/core": "workspace:*"
	},
	"dependencies": {
		"effect": "^3.19.14"
	},
	"devDependencies": {
		"@effect/eslint-plugin": "^0.3.2",
		"@types/node": "^25.0.3",
