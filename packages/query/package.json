{
	"name": "@effuse/query",
	"version": "0.1.0",
	"description": "Effuse Query handles loading, caching, and synchronizing server state. It provides reactive hooks for data fetching with built-in reliability.",
	"author": "Chris M. PÃ©rez",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/chrismichaelps/effuse.git"
	},
	"bugs": {
		"url": "https://github.com/chrismichaelps/effuse/issues"
	},
	"homepage": "https://github.com/chrismichaelps/effuse#readme",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
