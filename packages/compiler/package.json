{
	"name": "@effuse/compiler",
	"version": "1.0.3",
	"description": "An optimized JSX/TSX transformer that automatically handles reactive signal access, reducing boilerplate without impacting performance.",
	"author": "Chris M. PÃ©rez",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/chrismichaelps/effuse.git"
	},
	"bugs": {
		"url": "https://github.com/chrismichaelps/effuse/issues"
	},
	"homepage": "https://github.com/chrismichaelps/effuse#readme",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./vite": {
			"types": "./dist/vite/index.d.ts",
			"import": "./dist/vite/index.js"
		}
	},
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"dev": "tsc -w -p tsconfig.json",
		"clean": "rm -rf dist",
		"typecheck": "tsc --noEmit",
		"test": "vitest run || echo 'No tests'",
		"test:watch": "vitest"
	},
	"dependencies": {
		"@babel/parser": "^7.29.0",
		"@babel/traverse": "^7.29.0",
		"@babel/generator": "^7.29.1",
		"@babel/types": "^7.29.0",
		"effect": "^3.19.17"
	},
	"devDependencies": {
		"@types/babel__generator": "^7.27.0",
		"@types/babel__traverse": "^7.28.0",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"vitest": "^4.0.18"
	},
	"peerDependencies": {
		"vite": "^5.0.0 || ^6.0.0"
	},
	"peerDependenciesMeta": {
		"vite": {
			"optional": true
		}
	},
	"keywords": [
		"effuse",
		"compiler",
		"jsx",
		"signals",
		"reactivity",
		"vite-plugin",
		"babel",
		"transform"
	]
}
